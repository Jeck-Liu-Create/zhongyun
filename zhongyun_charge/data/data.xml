<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="seq_demo_sequence_sn" model="ir.sequence">
      <field name="name">zhongyun_charge_SN</field>
      <field name="code">zy.charge.sequence</field>
      <field name="active">True</field>
      <field name="prefix">SNZC%(y)s%(month)s%(day)s</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="mail_zhongyun_approval" model="mail.activity.type">
      <field name="name">运价审批</field>
      <field name="icon">fa-dollar</field>
      <field name="res_model">zy.charge</field>
      <!-- <field name="res_model_id" ref="zhongyun_charge.model_zy_charge"/> -->
    </record>
  
  </data>
</odoo>
