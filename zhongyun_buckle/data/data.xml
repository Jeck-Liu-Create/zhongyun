<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="seq_buckle_sequence_sn" model="ir.sequence">
      <field name="name">zhongyun_buckle_SN</field>
      <field name="code">zy.buckle.sequence</field>
      <field name="active">True</field>
      <field name="prefix">SNZB%(y)s%(month)s%(day)s</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="mail_zhongyun_buckle_approval" model="mail.activity.type">
      <field name="name">计量价格审批</field>
      <field name="icon">fa-dollar</field>
      <field name="res_model">zy.buckle</field>
      <!-- <field name="res_model_id" ref="zhongyun_buckle.model_zy_buckle"/> -->
    </record>
  
  </data>
</odoo>
