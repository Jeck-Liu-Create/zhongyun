<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="seq_goods_sequence_sn" model="ir.sequence">
      <field name="name">zhongyun_goods_SN</field>
      <field name="code">zy.goods.price.sequence</field>
      <field name="active">True</field>
      <field name="prefix">SNZG%(y)s%(month)s%(day)s</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="mail_zhongyun_goods_approval" model="mail.activity.type">
      <field name="name">货物价格审批</field>
      <field name="icon">fa-dollar</field>
      <field name="res_model">zy.goods.price</field>
      <!-- <field name="res_model_id" ref="zhongyun_goods.model_zy_goods_price"/> -->
    </record>
  
  </data>
</odoo>
